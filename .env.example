# Environment Variables for EasyMed
# Copy this file to .env and fill in your actual values

# MongoDB connection string
VITE_MONGODB_URI=mongodb://localhost:27017/easymed

# OpenAI API key for AI health assistant
VITE_OPENAI_API_KEY=sk-your-openai-api-key-here

# Optional: API base URL for backend services
VITE_API_BASE_URL=http://localhost:3000/api

# Optional: Environment (development, staging, production)
VITE_ENV=development